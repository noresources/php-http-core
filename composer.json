	}, 
	"extra": {
		"branch-alias": {
			"dev-1.0.x": "1.0.x-dev", 
			"dev-1.2.x": "1.2.x-dev", 
			"dev-main": "2.0.x-dev"
		}
	}, 
	"replace": {
		"noresources/ns-php-http-core": "2.0.0"
	}, 
	"support": {
		"issues": "https://github.com/noresources/php-http-core/issues"
