{
	"name": "noresources/http-core", 
	"description": "HTTP core", 
	"type": "library", 
	"license": "MIT", 
	"keywords": [
		"http", 
		"RFC 7230", 
		"RFC 7231"
	], 
	"authors": [
		{
			"name": "Renaud Guillard", 
			"email": "dev@nore.fr", 
			"role": "lead"
		}
	], 
	"autoload": {
